<!DOCTYPE html>
<html lang="en">
<%- include('partials/head2') %>

    <body>
        <div class="grid">
            <%- include('partials/clientandsupportnavbar') %>
                <div class="content_section">
                    <%- include('partials/clientnavbar') %>
                    <form action="/users/observationquery" method="post">
                        <fieldset>
                            
                            <legend>Hello User</legend>
                            
                            <fieldset>
                                <legend>Select from list of all observations</legend>
                                <select name="_id">
                                    <%users.forEach(function(obj){%>
                                        <option value="<%=obj._id%>"</option>
                                        <%=obj.title%>
                                       
                                    <%})%>
                                </select>
                                <br /><br />
                                <button>View Observation</button>
                            </fieldset>


                        </fieldset>
                    </form>
                </div>
                <%- include('partials/footer') %>
        </div>



    </body>

</html>