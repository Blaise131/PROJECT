<!DOCTYPE html>
<html lang="en">
<%- include('partials/head2') %>
<body>
    <div class="grid">
        <%- include('partials/clientandsupportnavbar') %>
            <div class="content_section">
                <%- include('partials/supportnavbar') %>
                <form action="queryuser" method="post">
                    <fieldset>
                        <legend>Query user details from database:</legend>
                        <p>
                            <label>Select User</label>
                            <select name="_id">
                                <%users.forEach(function(obj){ %>
                                    <option value="<%=obj._id%>"</option>
                                    <%=obj.firstName%>
                                    <%=obj.lastName%>
                                <%})%>
                                </select>
                        </p>
                        </p>
                        <button>Query user</button>
                    </fieldset>
                </form>
            </div>
            <div class="footer_section">----</div></div> 
            </div>
    </div>
    
</body>
</html>
